{
  "name": "easypoll-graphql",
  "description": "GraphQL API for EasyPoll",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "ISC",
  "author": "Ant√¥nio Junior <acsjunior1@gmail.com>",
  "scripts": {
    "prebuild": "npm install --production",
    "build": "tslint -c tslint.json -p tsconfig.json --fix",
    "postbuild": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "cross-env nodemon --watch \"src\" --exec \"tsc && ts-node dist/index.js\" -e ts,js",
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": " NODE_ENV=test npm run test -- --watch",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all --debug",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "lint": "eslint src/** --fix --ext .js,.ts"
  },
  "keywords": [],
  "engines": {
    "node": ">= 4.4.7",
    "npm": ">= 2.15.8"
  },
  "resolutions": {
    "apollo-link-dedup": "^1.0.13",
    "@types/graphql": "^14.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.7.0",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2"
  },
  "dependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.10.0",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tools": "^6.0.1",
    "graphql-tools-merge-typedefs": "^2.0.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.4",
    "sequelize-typescript": "^1.1.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "uuid": "^7.0.2"
  }
}
